{
  "version": 2,
  "builds": [
    {
      "src": "krishimitra_backend/krishimitra_backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "krishimitra_backend/krishimitra_backend/wsgi.py"
    }
  ],
  "env": {
    "VERCEL": "1"
  },
  "functions": {
    "krishimitra_backend/krishimitra_backend/wsgi.py": {
      "maxDuration": 30
    }
  }
}
