{
  "version": 2,
  "builds": [
    {
      "src": "krishimitra_backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "buildCommand": "bash build_files.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "krishimitra_backend/wsgi.py"
    }
  ]
}